@model List<User>
@{
    ViewData["Title"] = "Deneme";
}

@ViewBag.str
<h1>Deneme</h1>


<table class="table">
    @foreach (User user in Model)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.Email</td>
            <td>@user.UserName</td>
            <td>@user.PasswordHash</td>
            <td>@user.UserType</td>
        </tr>
    }
</table>

